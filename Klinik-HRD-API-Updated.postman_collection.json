					"name": "Login Admin",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"if (pm.response.code === 200) {",
									"    const response = pm.response.json();",
									"    pm.environment.set('token', response.token);",
									"    pm.test('Login successful', function () {",
									"        pm.expect(response.status).to.eql('success');",
									"        pm.expect(response.token).to.exist;",
